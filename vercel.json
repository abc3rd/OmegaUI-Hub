{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": true,
    "framework": null,
    "rewrites": [
      { "source": "/dashboard", "destination": "/dashboard.html" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "no-store, max-age=0" }
        ]
      }
    ]
  }