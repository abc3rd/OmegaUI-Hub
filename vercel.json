{
  "cleanUrls": true,
  "trailingSlash": true,
  "rewrites": [
    {
      "source": "/apps/:app/src/:path*",
      "destination": "/apps/:app/:path*"
    },
    {
      "source": "/apps/:app/assets/:path*",
      "destination": "/apps/:app/assets/:path*"
    },
    {
      "source": "/apps/:path*",
      "destination": "/apps/:path*/index.html"
    }
  ]
}