{
  "name": "UcpRequest",
  "type": "object",
  "properties": {
    "original_prompt": {
      "type": "string",
      "description": "The original user prompt"
    },
    "ucp_command": {
      "type": "string",
      "description": "The translated UCP command"
    },
    "original_tokens": {
      "type": "number",
      "description": "Token count before optimization"
    },
    "ucp_tokens": {
      "type": "number",
      "description": "Token count after optimization"
    },
    "savings_percentage": {
      "type": "number",
      "description": "Percentage of tokens saved"
    },
    "response_time_ms": {
      "type": "number",
      "description": "API response time in milliseconds"
    },
    "ai_response": {
      "type": "string",
      "description": "Response from AI"
    },
    "success": {
      "type": "boolean",
      "description": "Whether the request was successful"
    },
    "api_endpoint": {
      "type": "string",
      "description": "Which API was used"
    }
  },
  "required": [
    "original_prompt",
    "ucp_command"
  ]
}