{
  "name": "KeywordResearch",
  "type": "object",
  "properties": {
    "keyword": {
      "type": "string",
      "description": "The main keyword researched"
    },
    "url": {
      "type": "string",
      "description": "Website URL if analyzed"
    },
    "search_volume": {
      "type": "number",
      "description": "Monthly search volume"
    },
    "difficulty": {
      "type": "number",
      "description": "Keyword difficulty score (0-100)"
    },
    "cpc": {
      "type": "number",
      "description": "Cost per click"
    },
    "competition": {
      "type": "string",
      "enum": [
        "Low",
        "Medium",
        "High"
      ],
      "description": "Competition level"
    },
    "intent": {
      "type": "string",
      "enum": [
        "informational",
        "navigational",
        "transactional",
        "commercial"
      ],
      "description": "Search intent"
    },
    "related_keywords": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "description": "Related keywords data"
    },
    "long_tail_keywords": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "description": "Long-tail keywords data"
    },
    "questions": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "description": "People also ask questions"
    },
    "clusters": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "description": "Keyword clusters"
    },
    "competitors": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "description": "Competitor analysis"
    },
    "serp_features": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "description": "SERP features present"
    },
    "content_ideas": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "description": "Content recommendations"
    },
    "analytics": {
      "type": "object",
      "description": "Analytics and metrics"
    },
    "extracted_keywords": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "description": "Keywords extracted from URL analysis"
    },
    "research_date": {
      "type": "string",
      "format": "date-time",
      "description": "When the research was performed"
    }
  },
  "required": [
    "keyword"
  ]
}