{
  "version": 2,
  "builds": [
    {
      "src": "cloud-connect/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://glytch-backend-production.up.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/cloud-connect/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://*.gohighlevel.com"
        }
      ]
    }
  ]
}